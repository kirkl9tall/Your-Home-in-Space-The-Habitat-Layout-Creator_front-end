# 3D Tiles Configuration
# Provider: "google" | "cesium" | "custom"
VITE_TILES_PROVIDER=google

# Google Photorealistic 3D Tiles (requires API key and billing)
VITE_GOOGLE_MAPS_API_KEY=__YOUR_GOOGLE_KEY__

# Cesium Ion (requires token and asset URL)
VITE_CESIUM_ION_TOKEN=__YOUR_CESIUM_TOKEN__
VITE_CESIUM_ION_URL=https://assets.cesium.com/<assetId>/tileset.json

# Custom Tiles URL
VITE_CUSTOM_TILES_URL=https://example.com/tiles/tileset.json